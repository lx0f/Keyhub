    font-weight: normal;
    padding: 8px;
    margin-left: -12px;
    margin-right: 6px;
  }

  .whitebuttondemo a:hover {
    color: #fff;
  }

  .whitebuttondemo span {
    background: #444;
    color: #fff;
    display: block;
    font-size: 12px;
    font-family: 'Arial', Verdana, sans-serif;
    height: 40px;
    line-height: 40px;
    text-align: center;
    width: 200px;
    z-index: 1;

    font-weight: bold;
  }

  .whitebuttondemo .up {
    background: #444;
