<style>
  body {
    background-color: #f1f1f2;
  }

  .tile {
    width: 80%;
    margin: 60px auto;
  }

  #tile-1 .tab-pane {
    padding: 15px;
    height: 80px;
  }


  #tile-1 .nav-tabs {
    position: relative;
    border: none !important;
    background-color: #fff;
